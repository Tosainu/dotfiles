snippet main int main() { ... }
	int main() {
		${0}
	}
snippet mainarg int main(int argc, char** argv) { ... }
	int main(int argc, char** argv) {
		${0}
	}
snippet inc #include <...>
	#include <${1:stdio.h}>
snippet Inc #include "..."
	#include "${1:`vim_snippets#Filename("$1.h")`}"
snippet ifndef #ifndef ... #define ... #endif
	#ifndef $1
	#define ${1:SYMBOL} ${2:value}
	#endif // $1
